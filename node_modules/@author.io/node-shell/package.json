{
  "name": "@author.io/node-shell",
  "version": "1.8.2",
  "description": "A micro-framework for creating CLI-like experiences. This supports Node.js and browsers.",
  "main": "index.js",
  "keywords": [
    "cli",
    "args",
    "arg",
    "shell"
  ],
  "author": "Corey Butler",
  "license": "MIT",
  "type": "module",
  "files": [
    "*.js"
  ],
  "engines": {
    "node": ">=13.5.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "_*",
      "_**/*",
      ".**/*",
      "node_modules",
      "karma.conf.js",
      "karma.conf.cjs",
      "build.js"
    ],
    "globals": [
      "window",
      "global",
      "globalThis"
    ]
  },
  "dependencies": {
    "@author.io/arg": "^1.3.8",
    "@author.io/table": "^1.0.3"
  },
  "module": "index.js",
  "devDependencies": {
    "@author.io/node-shell-debug": "1.8.2"
  }
}