{
  "name": "@ngnjs/libcrypto",
  "version": "1.0.0",
  "description": "A simple cryptography plugin for NGN.",
  "type": "module",
  "author": "Corey Butler",
  "private": false,
  "license": "MIT",
  "homepage": "https://github.com/ngnjs/libcrypto",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngnjs/libcrypto.git"
  },
  "bugs": {
    "url": "https://github.com/ngnjs/libcrypto/issues"
  },
  "main": "./src/index.js",
  "module": "index.js",
  "exports": {
    ".": {
      "import": "./index.js"
    }
  },
  "browser": "./index.js",
  "sideEffects": false,
  "keywords": [
    "crypto",
    "cryptography",
    "key",
    "public",
    "private",
    "encrypt",
    "encryption",
    "decrypt",
    "decryption",
    "sign",
    "verify",
    "hotp",
    "totp",
    "rsa",
    "ecdsa",
    "ec"
  ],
  "peerDependencies": {
    "ngn": "^2.0.0-alpha.10"
  },
  "devDependencies": {
    "@author.io/dev": "^1.1.5",
    "@ngnjs/libcrypto-debug": "1.0.0"
  },
  "dev": {
    "mode": "source",
    "replace": {
      "<#REPLACE_VERSION#>": "package.version"
    },
    "autoimport": [
      "import NGN from 'ngn'"
    ],
    "ci": {
      "verbose": true,
      "peer": true,
      "buildoption": {
        "preserveEntrySignatures": true
      },
      "alias": {
        "ngn": "/node_modules/ngn/index.js",
        "@ngnjs/libcrypto": "/app/.dist/libcrypto/index.js"
      }
    },
    "source": {
      "volume": [
        "../core/.dist/ngn:/node_modules/ngn",
        "../core/src:/source/ngn"
      ],
      "buildoption": {
        "preserveEntrySignatures": true
      },
      "alias": {
        "ngn": "/node_modules/ngn/index.js",
        "@ngnjs/libcrypto": "/app/src/index.js"
      }
    }
  },
  "standard": {
    "globals": [
      "globalThis",
      "window",
      "global"
    ]
  },
  "dependencies": {}
}